{
  "unstable": ["temporal", "webgpu"],
  "nodeModulesDir": "manual",
  "imports": {
    "@anthropic-ai/claude-code": "npm:@anthropic-ai/claude-code@^1.0.60",
    "@bfmono/": "./",
    "@bolt-foundry/bolt-foundry": "./packages/bolt-foundry/bolt-foundry.ts",
    "@bolt-foundry/bolt-foundry/": "./packages/bolt-foundry/",
    "@bolt-foundry/logger": "./packages/logger/logger.ts",
    "@bolt-foundry/get-configuration-var": "./packages/get-configuration-var/get-configuration-var.ts",

    "@b-fuze/deno-dom": "jsr:@b-fuze/deno-dom@^0.1.49",
    "@deno/cache-dir": "jsr:@deno/cache-dir@^0.22.3",
    "@deno/dnt": "jsr:@deno/dnt@^0.41.3",
    "@iso": "./apps/boltFoundry/__generated__/__isograph/iso.ts",
    "@iso/": "./apps/boltFoundry/__generated__/__isograph/",
    "@iso-bfc": "./apps/boltfoundry-com/__generated__/__isograph/iso.ts",
    "@iso-bfc/": "./apps/boltfoundry-com/__generated__/__isograph/",
    "@isograph/babel-plugin": "npm:@isograph/babel-plugin@^0.3.1",
    "@isograph/react": "npm:@isograph/react@^0.3.1",
    "@lexical/mark": "npm:@lexical/mark@^0.33.0",
    "@lexical/react": "npm:@lexical/react@^0.33.0",
    "@lexical/selection": "npm:@lexical/selection@^0.33.0",
    "@lexical/text": "npm:@lexical/text@^0.33.0",
    "@lexical/utils": "npm:@lexical/utils@^0.33.0",
    "@mdx-js/esbuild": "npm:@mdx-js/esbuild@^3.1.0",
    "@neondatabase/serverless": "npm:@neondatabase/serverless@^1.0.1",
    "@openai/openai": "jsr:@openai/openai@^4.78.1",
    "@pothos/core": "npm:@pothos/core@^4.7.3",
    "@pothos/plugin-relay": "npm:@pothos/plugin-relay@^4.4.2",
    "@simplewebauthn/browser": "jsr:@simplewebauthn/browser@^13.1.0",
    "@simplewebauthn/server": "jsr:@simplewebauthn/server@^13.1.1",
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/async": "jsr:@std/async@^1.0.13",
    "@std/cli": "jsr:@std/cli@^1.0.20",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/flags": "jsr:@std/flags@^0.224.0",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/front-matter": "jsr:@std/front-matter@^1.0.9",
    "@std/fs": "jsr:@std/fs@^1.0.18",
    "@std/http": "jsr:@std/http@^1.0.12",
    "@std/io": "jsr:@std/io@^0.225.2",
    "@std/path": "jsr:@std/path@^1.1.0",
    "@std/streams": "jsr:@std/streams@^1.0.10",
    "@std/testing": "jsr:@std/testing@^1.0.9",
    "@std/toml": "jsr:@std/toml@^1.0.4",
    "@types/google.accounts": "npm:@types/google.accounts@^0.0.17",
    "@types/matter-js": "npm:@types/matter-js@^0.19.8",
    "@types/pg": "npm:@types/pg@^8.15.4",
    "assemblyai": "npm:assemblyai@^4.14.0",
    "esbuild": "https://deno.land/x/esbuild/mod.js",
    "graphql": "npm:graphql@^16.11.0",
    "graphql-relay": "npm:graphql-relay@^0.10.2",
    "graphql-yoga": "npm:graphql-yoga@^5.14.0",
    "@graphql-tools/schema": "npm:@graphql-tools/schema@^10.0.6",
    "lexical": "npm:lexical@^0.33.0",
    "marked": "npm:marked@^16.0.0",
    "matter-js": "npm:matter-js@^0.20.0",
    "pg": "npm:pg@^8.16.3",
    "posthog-js": "npm:posthog-js@^1.256.2",
    "posthog-node": "npm:posthog-node@^5.1.1",
    "puppeteer": "npm:puppeteer@^24.11.2",
    "puppeteer-core": "npm:puppeteer-core@^24.11.2",
    "react": "npm:react@^19.1.0",
    "react-dom": "npm:react-dom@^19.1.0",
    "sqlite": "node:sqlite",

    "apps/": "./apps/",
    "experimental/": "./experimental/",
    "infra/": "./infra/",
    "lib/": "./lib/",
    "packages/": "./packages/",
    "util/": "./util/",
    "posthog-js/react": "./node_modules/posthog-js/react/dist/esm/index.js",
    "react/jsx-runtime": "npm:react/jsx-runtime",
    "content/": "./content/",
    "build/content/": "./build/content/",
    "docs/": "./docs/",
    "build/docs/": "./build/docs/",
    "static/": "./static/",
    "tmp/": "./tmp/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:react",
    "jsxImportSourceTypes": "npm:@types/react@^19",

    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "webworker.importscripts",
      "webworker.iterable",
      "deno.ns",
      "deno.unstable"
    ]
  },
  "include": [
    "app",
    "infra",
    "lib",
    "documentation",
    "packages",
    "sites",
    "docs",
    "agents"
  ],
  "exclude": [
    ".cache",
    ".sl",
    ".bft/**",
    ".direnv",
    ".git",
    ".deno/**",
    "build",
    "static",
    "tmp",
    "**/__TEMPLATE__.*",
    "**/*.disabled.*",
    "thirdParty",
    "**/npm",
    "node_modules/**",
    "**/node_modules/**",
    "examples/**",
    "**/dist/**",
    "**/.vite/**",
    "packages/aibff",
    "apps/contacts/**"
  ],
  "lint": {
    "exclude": [
      "**/__generated__/**",
      "**/__isograph/**",
      "examples/**",
      "**/dist/**"
    ],
    "plugins": ["./infra/lint/bolt-foundry.ts"],

    "rules": {
      "include": [
        "bolt-foundry/no-bfnodespec-first-static",
        "bolt-foundry/no-underscore-logger-when-used",
        "bolt-foundry/no-logger-set-level",
        "bolt-foundry/prefer-generic-array-syntax",
        "bolt-foundry/ensure-file-ends-with-newline",
        "bolt-foundry/ts-expect-error-description",
        "bolt-foundry/no-parent-directory-traversal",
        "bolt-foundry/no-cornercased-imports",
        "no-slow-types",
        "no-console",
        "no-external-import",
        "no-self-compare",
        "no-sync-fn-in-async-fn",
        "verbatim-module-syntax",
        "no-eval"
      ]
    }
  },
  "fmt": {
    "exclude": [
      "**/__generated__/**",
      "**/*.json",
      "**/*.ipynb",
      "**/.ipynb_checkpoints",
      "**/node_modules/**",
      "**/dist/**"
    ]
  },

  "lock": {
    "path": "./deno.lock"
  },
  "workspace": [
    "apps/aibff/gui",
    "apps/boltfoundry-com",
    "packages/bolt-foundry",
    "packages/logger",
    "packages/get-configuration-var"
  ],
  "vendor": true
}
